// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["name"],
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"name": {
			"bsonType": "string",
			"description": "类别名称",
			"label": "名称",
			"trim": "both"
		},
		"description": {
			"bsonType": "string",
			"description": "类别描述",
			"label": "描述"
		},
		"create_date": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"label": "创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		}
	}
}
